    "express": "4.17.1",
    "fast-csv": "4.0.2",
    "fetch-with-proxy": "2.0.1",
    "fs-extra": "9.0.0",
    "geckodriver": "1.19.1",
    "http": "0.0.1-security",
    "import-local": "3.0.2",
    "jest": "25.5.4",
    "jest-cli": "25.1.0",
    "jest-html-reporters": "1.2.1",
    "jest-runner-groups": "1.1.1",
    "jest-sonar-reporter": "2.0.0",
    "jest-stare": "1.26.0",
    "jsoneditor": "8.6.6",
    "mkdirp": "1.0.4",
    "mocha": "7.1.1",
    "mocha-jenkins-reporter": "0.4.3",
