  "jest": {
    "runner": "groups",
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  },
  "scripts": {
    "eslint": "npx eslint *.js **/src/**/*.js **/src/**/*.js",
    "eslint:files": "npx eslint *.js **/src/**/*.js **/src/**/*.js | grep ^[\\w/].*\\.js",
    "eslint:files:open": "code $(npx eslint *.js **/src/**/*.js **/src/**/*.js | grep ^[\\w/].*\\.js)",
    "prestart": "npm install && npm install -g chromedriver --force",
    "test": "jest --detectOpenHandles ",
    "itn01": "jest --detectOpenHandles --ienv=itn01 --configFile=config.min.json",
    "itn02": "jest --detectOpenHandles --ienv=itn02 --configFile=config.min.json",
    "itn02-lw-p1m1ch1c1": "jest --configFile=config.min.json   --group=provide1 --ienv=itn02",
    "itn01-hs-p2m3c2": "jest --detectOpenHandles --ienv=itn01 --configFile=config.min.json --group=provide1 --group=move3 --group=cease2",
    "itn01-lw-p1m1c1": "jest --detectOpenHandles --ienv=itn01 --configFile=config.min.json --group=provide1 --group=move1 --group=testcease1",
    "itn01-hs-p2ch1c2": "jest --detectOpenHandles --ienv=itn01 --configFile=config.min.json --group=provide2 --group=change1 --group=cease2",
    "itn01-hs-psch1": "jest --detectOpenHandles --ienv=itn01 --configFile=config.min.json --group=providesecure --group=change1",
    "itn02-ch1ch2": "jest --detectOpenHandles --ienv=itn02 --configFile=config.min.json  --group=change1 --group=change2",
    "itn02-lw-p1m1c1": "jest --detectOpenHandles --ienv=itn02 --configFile=config.min.json --group=provide1 --group=move1 --group=testcease1",
    "itn02-lw-p1c1": "jest --detectOpenHandles --ienv=itn02 --configFile=config.min.json --group=provide1 --group=cease1",
    "itn02-hs-p2m3c2": "jest --detectOpenHandles --ienv=itn02 --configFile=config.min.json --group=provide2 --group=move3 --group=testcease2",
    "par-all": "start npm run itn02-lw-p1c1 && start npm run itn02-hs-p2m3c2"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "sonar-test-report.xml",
    "indent": 4
  },
  "devDependencies": {
    "eslint": "7.4.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "prettier": "2.0.5",
    "jest-expect-message": "1.0.2"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "author": "Pankaj Gupta",
  "license": "ISC",
  "testResultsProcessor": "jest-html-reporters"
}